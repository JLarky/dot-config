{
  // "TypeScriptImporter.showNamespaceOnImports": false,
  "shellformat.path": "/opt/shfmt_v2.5.1_linux_amd64",
  // "editor.parameterHints": false,
  "typescript.autoImportSuggestions.enabled": true,
  "editor.smoothScrolling": true,

  // https://newbedev.com/prevent-autocomplete-in-visual-studio-code
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "off",
    "other": "off"
  },
  "editor.quickSuggestionsDelay": 10,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.wordBasedSuggestions": false,
  // for copilot
  "editor.inlineSuggest.enabled": true,
  // end

  "editor.mouseWheelScrollSensitivity": 0.8,
  // "editor.renderLineHighlight": "line",
  "workbench.colorCustomizations": {
    // "editorCursor.foreground": "#b6a0ff",
    "editorBracketMatch.background": "#802200",
    "editorBracketMatch.border": "#ffffff00",
    "editor.lineHighlightBackground": "#cccccc11",
    "statusBar.debuggingBackground": "#4a4e8660"
  },
  // "vim.disableAnnoyingNeovimMessage": true,
  // "vim.otherModesKeyBindingsNonRecursive": [
  //     {
  //         "before": ["Z", "Q"],
  //         "after": [],
  //         "commands": [
  //             {
  //                 "command": "workbench.action.closeActiveEditor",
  //                 "args": []
  //             }
  //         ]
  //     },
  //     {
  //         "before": ["Z", "Z"],
  //         "after": [],
  //         "commands": [
  //             {
  //                 "command": "workbench.action.files.save",
  //                 "args": []
  //             }
  //         ]
  //     }
  // ],
  // "vim.useSystemClipboard": true,
  // "vim.useCtrlKeys": false,
  "files.insertFinalNewline": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "amVim.bindCtrlCommands": false,
  "amVim.useSystemClipboard": true,
  "window.titleBarStyle": "custom",
  "breadcrumbs.enabled": true,
  "editor.wordWrap": "on",
  "[shellscript]": {
    "editor.defaultFormatter": "shakram02.bash-beautify"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.rendererType": "experimentalWebgl",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    ".pnp.js": true,
    ".yarn/**": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.semanticHighlighting.enabled": true,
  "cSpell.userWords": [
    "Dropzone",
    "fogbender",
    "helpdesk",
    "immer",
    "jotai",
    "journaling",
    "microbundle",
    "openid",
    "paseto",
    "Paseto",
    "pulumi",
    "refetch",
    "semibold",
    "subpage",
    "unfetch",
    "unmount",
    "use",
    "use immer",
    "uuidv",
    "Yaroslav"
  ],
  "emmet.showExpandedAbbreviation": "never",
  "editor.gotoLocation.multipleDefinitions": "gotoAndPeek",
  "editor.gotoLocation.multipleTypeDefinitions": "gotoAndPeek",
  "workbench.editor.revealIfOpen": true,
  "files.hotExit": "onExitAndWindowClose",
  "vim.useSystemClipboard": false,
  "vim.useCtrlKeys": true,
  "vim.startInInsertMode": false,
  "vim.handleKeys": {
    "<C-PgUp>": false,
    "<C-v>": false,
    "<C-x>": false,
    "<C-c>": false,
    "<C-d>": false
  },
  "editor.formatOnSave": true,
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "explorer.openEditors.visible": 0,
  "grammarly.domain": "technical",
  "grammarly.hideUnavailablePremiumAlerts": true,
  "elixirLS.fetchDeps": false,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "warmUp.changeCount": "15",
  "warmUp.switchProgrammingLanguage": "typescript",
  "warmUp.changeTypingMode": "words (fixed amount)",
  "git.autofetch": true,
  "git.mergeEditor": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "One Monokai Darker",
  "files.associations": {
    "*.astro": "astro"
  },
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "editor.stickyScroll.enabled": true,
  "terminal.integrated.fontSize": 10,
  "editor.fontSize": 14,
  // https://github.com/microsoft/vscode-eslint/issues/1199
  "eslint.rules.customizations2": [
    { "rule": "no-*", "override": "info" }, // override all no-* rules to info
    { "rule": "!no-*", "override": "warn" }, // override all other (not no-*) rules to warn
    { "rule": "*console*", "reset": "original" }, // reset all *console* rules to the original config
    { "rule": "*syntax*", "reset": "lower" }, // reset all *syntax* rules to one lower than the original config
    { "rule": "*prettier*", "override": "info" }
  ],
  // https://github.com/microsoft/vscode-eslint/pull/1164
  "eslint.rules.customizations": [
    {
      "rule": "*",
      "severity": "downgrade"
    }
  ]
}
